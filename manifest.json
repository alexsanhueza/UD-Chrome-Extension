{
  "manifest_version": 2,

  "name": "Urban Dictionary Lookup Plugin",
  "description": "This will aid you in your shameless linguistic searching my friends",
  "version": "1.0",

  "browser_action": {
    "default_icon": "UDicon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["stretch.js"]
  },
  "permissions": ["contextMenus"],
  "icons": {
    "16": "UDicon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
